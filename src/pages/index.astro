---
// Component Imports
import Home from "../components/Home.astro";

// Component Script:
let title = "Miao";
---

<html lang="en">
  <head>
    <meta charset="UTF-8" />
    <meta name="viewport" content="width=device-width" />
    <title>{title}</title>

    <link rel="icon" type="image/x-icon" href="/favicon.ico" />
    <style global>
      @import "../styles/global.css";
    </style>
  </head>
  <body>
    <svg
      viewBox="0 0 100 100"
      xmlns="http://www.w3.org/2000/svg"
      style="display: none"
      ><defs>
        <path
          id="icon-external-link"
          fill="transparent"
          stroke="currentColor"
          stroke-width="10"
          d="m43,35H5v60h60V57M45,5v10l10,10-30,30 20,20 30-30 10,10h10V5z"
        ></path>
      </defs></svg
    >
    <main>
      <Home />
    </main>
  </body>
</html>
